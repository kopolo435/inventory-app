<!DOCTYPE html>
<html lang="en">
  <%- include("head_layout",{title}) %>
  <body>
    <h1><%= title %></h1>
    <p>Lista de usuarios:</p>
    <ul>
      <% allUsers.forEach(user => { %>
      <li>
        <a href="<%= user.url %>"><%= user.fullname %></a>
        <p><%= user.cellphone %></p>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
